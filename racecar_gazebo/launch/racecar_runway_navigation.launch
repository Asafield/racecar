<?xml version="1.0"?>
<launch>
  <!-- Launch the racecar -->
  <include file="$(find racecar_gazebo)/launch/racecar.launch">
  <arg name="world_name" value="obj"/>
  </include>

  <!-- servo node -->
  <!-- <node pkg="racecar_control" type="servo_commands_zfk_1.py" name="servo_commands" output="screen"> -->
  <!-- <node pkg="racecar_control" type="servo_commands_self.py" name="servo_commands" output="screen">
    <remap from="/racecar/ackermann_cmd_mux/output" to="/vesc/low_level/ackermann_cmd_mux/output"/>
  </node> -->

  <node name="map_server" pkg="map_server" type="map_server" args="$(find racecar_gazebo)/map/map.yaml" />

  <!--使用amcl进行定位-->
  <include file="$(find racecar_gazebo)/launch/racecar_amcl.launch">
    <rosparam file= "$(find racecar_gazebo)/config/amcl_params.yaml)"/>
  </include>


  <!-- Launch the built-map -->
  <!--Launch the move base with time elastic band-->
  <param name="/use_sim_time" value="true"/>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find racecar_gazebo)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find racecar_gazebo)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find racecar_gazebo)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find racecar_gazebo)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find racecar_gazebo)/config/teb_local_planner_params.yaml" command="load" />
    <!-- <rosparam file="$(find racecar_gazebo)/config/teb_local_planner_params.yaml" command="load" /> -->

    <param name="base_global_planner" value="global_planner/GlobalPlanner" />
    <param name="planner_frequency" value="0.01" />
    <param name="planner_patience" value="5.0" />
    <!--param name="use_dijkstra" value="false" /-->
    
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <param name="controller_frequency" value="20.0" />
    <param name="controller_patience" value="15.0" />

    <param name="clearing_rotation_allowed" value="false" />
  </node>
  <!--rviz-->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find racecar_gazebo)/config/racecar_rviz.rviz"/>
  <!--可视化路径-->
  <!-- <node pkg="racecar_gazebo" type="visualize_path.py" name="visualize_path" /> -->
  
  <!-- ************cmd_vel变换输出************ -->
  <node name="servo_commands" pkg="racecar_gazebo" type="servo_commands.py" />
	<!-- 比赛计时器 -->
  <node pkg="racecar_gazebo" type="race_timer.py" name="race_timer" output="screen"/>
</launch>
